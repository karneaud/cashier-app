{
  "name": "@quasar/quasar-app-extension-testing-unit-jest",
  "version": "2.2.5",
  "license": "MIT",
  "description": "A Quasar App Extension for running Jest tests",
  "contributors": [
    "Paolo Caleffi",
    "Daniel Thompson-Yvetot"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/quasarframework/quasar-testing"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --declaration --declarationDir dist/types && copyfiles -f src/helpers/stub.css dist",
    "deploy": "yarn install && yarn build && yarn publish --tag qv1",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test": "snyk test && echo \"No test specified\" && exit 0"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@types/jest": "^26.0.23",
    "@vue/test-utils": "^1.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "chai": "^4.3.4",
    "concurrently": "^6.2.0",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "json5": "^2.2.0",
    "loader-utils": "^1.2.3",
    "promise": "^8.1.0",
    "ts-jest": "^26.5.4",
    "vue-jest": "^3.0.7"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "quasar": "^1.15.16",
    "snyk": "^1.180.1",
    "typescript": "^4.2.4",
    "vue": "^2.6.12"
  }
}
